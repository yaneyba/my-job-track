# Environment variables for MyJobTrack

# API Configuration
VITE_API_URL=http://localhost:8787
VITE_API_KEY=your-api-key-here
VITE_USE_API_PROVIDER=true
VITE_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# MVP Mode Settings
VITE_USE_MVP_MODE=true

# Cache Management
VITE_ENABLE_AUTO_UPDATES=false

# Demo User Credentials
VITE_DEMO_EMAIL=demo@yourdomain.com
VITE_DEMO_PASSWORD=YourSecurePassword123!

# Admin Settings
VITE_ADMIN_PASSWORD=YourAdminPassword123!
